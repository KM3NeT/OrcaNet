[run]
source = orcanet

[report]
include =
    orcanet/*
    # orcanet_contrib/*
omit =
    orcanet/tests/*
    orcanet/builder_util/multi_gpu/*
exclude_lines =
    pragma: no cover
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.debug:
    if settings.DEBUG

